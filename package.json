{
  "name": "service-listener",
  "version": "0.1.7",
  "engines": {
    "node": "^8"
  },
  "description": "A script to listen to kubernetes to automatically identify running deployments/services",
  "homepage": "https://github.com/WealthWizardsEngineering/service-listener",
  "repository": {
    "type": "git",
    "url": "https://github.com/WealthWizardsEngineering/service-listener.git"
  },
  "scripts": {
    "start": "node service-listener.js",
    "test:unit": "tape 'test/unit/**/*.spec.js'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html --reporter=lcov npm run test:unit",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "dependency-check": "rm -f package-lock.json && synp --source-file yarn.lock && npm --production audit --registry=https://registry.npmjs.org --json | node ./test/audit",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Wealth Wizards",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "lodash.capitalize": "^4.2.1",
    "nyc": "13.3.0",
    "proxyquire": "2.1.0",
    "ramda": "0.26.1",
    "sinon": "7.2.4",
    "synp": "1.3.2",
    "tape": "4.10.1"
  },
  "dependencies": {
    "bunyan": "1.8.12",
    "co": "4.6.0",
    "consul": "0.34.1",
    "envalid": "4.2.2",
    "json-stream": "^1.0.0",
    "kubernetes-client": "^6.8.3",
    "request": "^2.82.0",
    "request-promise-native": "1.0.7",
    "snyk": "^1.189.0"
  },
  "snyk": true
}

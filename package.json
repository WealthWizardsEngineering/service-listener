{
  "name": "service-listener",
  "version": "0.0.1",
  "engines": {
    "node": "6.2.0"
  },
  "description": "A script to listen to kubernetes to automatically identify the wealthwizards services",
  "scripts": {
    "start": "node service-listener.js",
    "test:unit": "tape 'test/unit/**/*.spec.js'",
    "test:unit:watch": "nodemon -q -x 'npm run test:unit | tap-spec --silent'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html --reporter=lcov npm run test:unit",
    "test:unit:report": "npm run test:unit:coverage | tee /dev/stderr | node_modules/tap-xunit/bin/tap-xunit > unit-test-report.xml",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js"
  },
  "author": "Wealth Wizards",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "envalid": "2.2.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.1",
    "nsp": "2.6.1",
    "nyc": "8.3.1",
    "proxyquire": "1.7.10",
    "sinon": "1.17.4",
    "sinon-as-promised": "4.0.0",
    "tape": "4.6.0"
  },
  "dependencies": {
    "json-stream": "^1.0.0",
    "kubernetes-client": "^3.8.0",
    "request": "^2.79.0",
    "request-promise-native": "1.0.3",
    "ww-logging": "0.3.0"
  }
}
